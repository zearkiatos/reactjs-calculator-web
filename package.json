{
  "name": "reactjs-calculator-web",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "description": "This is a web calculator 🧮 with reactjs ⚛️",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.0",
    "firebase": "^8.6.8",
    "lodash.words": "^4.2.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:watch": "nodemon start",
    "dev": "npm run start:watch",
    "build": "react-scripts build",
    "build:ci": "CI=false npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "firebase:deploy": "firebase deploy",
    "prefirebase:deploy": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:dev": "concurrently --kill-others \"npm run dev\" \"npm run storybook\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/preset-create-react-app": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-webpack5": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^8.2.0",
    "eslint-plugin-storybook": "^0.6.12",
    "nodemon": "^2.0.15",
    "storybook": "^7.0.27",
    "webpack": "^5.88.1"
  }
}
